package com.prac;
import static org.junit.jupiter.api.Assertions.assertEquals;

//import static org.junit.jupiter.api.Assertions.assetEquals;
import org.junit.jupiter.api.Test;
public class CalcuTest {
	public void addTest() {
		int res=Calculator.add(10, 30);
	}
	
	@Test
	public void revTest() {
		String res=Calculator.rev("Krish");
		assertEquals("hsirK",res);
	}
	
	@Test
	public void reverseTestNull() {
		String res=Calculator.rev(null);
		assertEquals("llun",res);
	}
}
