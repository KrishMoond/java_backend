package com.prac;

import java.util.*;
public class Calculator {
	public static int add(int a, int b) {
		return(a+b);
	}
	
	public static String rev(String a) {
//		StringBuilder sb= new StringBuilder(a);
//		sb.reverse();
//		return(sb.toString());
		String rev="";
		for(int i=a.length()-1;i>=0;i--) {
			rev+=a.charAt(i);
		}
		
		return rev;
	}
}
